Cell[BoxData[{
 StyleBox[
  RowBox[{
   RowBox[{"Lagrange", "[", 
    RowBox[{"x0_", ",", " ", "f0_"}], "]"}], " ", ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"xi", " ", "=", " ", "x0"}], ",", " ", 
       RowBox[{"fi", " ", "=", " ", "f0"}], ",", " ", "n", ",", "m", ",", " ",
        "polynomial"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"n", " ", "=", " ", 
       RowBox[{"Length", "[", "xi", "]"}]}], ";", " ", 
      RowBox[{"m", " ", "=", " ", 
       RowBox[{"Length", "[", "fi", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", " ", 
       RowBox[{
        RowBox[{"n", " ", "\[NotEqual]", " ", "m"}], ",", " ", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
         "Print", "[", 
          "\"\<List of points and function values are not of same size\>\"", 
          "]"}], ";", " ", 
         RowBox[{"Return", "[", "]"}], ";"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
        RowBox[{"i", "\[LessEqual]", "n"}], ",", " ", 
        RowBox[{"i", "++"}], ",", " ", 
        RowBox[{
         RowBox[{
          RowBox[{"L", "[", 
           RowBox[{"i", ",", " ", "x_"}], "]"}], " ", "=", " ", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             UnderoverscriptBox["\[Product]", 
              RowBox[{"j", "=", "1"}], 
              RowBox[{"i", "-", "1"}]], 
             FractionBox[
              RowBox[{"x", " ", "-", " ", 
               RowBox[{"xi", "[", 
                RowBox[{"[", "j", "]"}], "]"}]}], 
              RowBox[{
               RowBox[{"xi", "[", 
                RowBox[{"[", "i", "]"}], "]"}], "-", 
               RowBox[{"xi", "[", 
                RowBox[{"[", "j", "]"}], "]"}]}]]}], ")"}], " ", 
           RowBox[{"(", 
            RowBox[{
             UnderoverscriptBox["\[Product]", 
              RowBox[{"j", "=", " ", 
               RowBox[{"i", "+", "1"}]}], "n"], 
             FractionBox[
              RowBox[{"x", " ", "-", " ", 
               RowBox[{"xi", "[", 
                RowBox[{"[", "j", "]"}], "]"}]}], 
              RowBox[{
               RowBox[{"xi", "[", 
                RowBox[{"[", "i", "]"}], "]"}], "-", 
               RowBox[{"xi", "[", 
                RowBox[{"[", "j", "]"}], "]"}]}]]}], ")"}]}]}], ";"}]}], 
       "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"polynomial", "[", "x_", "]"}], " ", "=", " ", 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"k", "=", "1"}], "n"], 
        RowBox[{
         RowBox[{"L", "[", 
          RowBox[{"k", ",", "x"}], "]"}], " ", "*", " ", 
         RowBox[{"fi", "[", 
          RowBox[{"[", "k", "]"}], "]"}]}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", 
       RowBox[{"polynomial", "[", "x", "]"}], "]"}], ";"}]}], "]"}]}],
  FontFamily->"Courier New",
  FontSize->18], "\[IndentingNewLine]", 
 StyleBox[
  RowBox[{
   RowBox[{"nodes", " ", "=", " ", 
    RowBox[{"{", 
     RowBox[{"0", ",", "1", ",", "3"}], "}"}]}], ";", " ", 
