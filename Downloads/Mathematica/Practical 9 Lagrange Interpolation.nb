    RowBox[{"{", 
     RowBox[{"1", ",", "3", ",", "55"}], "}"}]}], ";"}],
  FontFamily->"Courier New",
  FontSize->18], "\[IndentingNewLine]", 
 StyleBox[
  RowBox[{
   RowBox[{"lagrangepolynomial", "[", "x_", "]"}], " ", "=", " ", 
   RowBox[{"Lagrange", "[", 
    RowBox[{"nodes", ",", "values"}], "]"}]}],
  FontFamily->"Courier New",
  FontSize->18]}], "Input",
