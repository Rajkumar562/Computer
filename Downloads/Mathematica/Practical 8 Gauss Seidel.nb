Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"GaussSeidel", "[", 
    RowBox[{"A0_", ",", "B0_", ",", "X0_", ",", " ", "max_"}], "]"}], " ", ":=",
    " ", "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"A", " ", "=", " ", 
        RowBox[{"N", "[", "A0", "]"}]}], ",", " ", 
       RowBox[{"B", " ", "=", " ", 
        RowBox[{"N", "[", "B0", "]"}]}], ",", " ", "i", ",", "j", ",", 
       RowBox[{"k", " ", "=", " ", "0"}], ",", " ", 
       RowBox[{"n", " ", "=", " ", 
        RowBox[{"Length", "[", "X0", "]"}]}], ",", " ", 
       RowBox[{"X", " ", "=", " ", "X0"}], ",", " ", 
       RowBox[{"Xk", "=", " ", "X0"}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Print", "[", 
       RowBox[{
        SubscriptBox["\"\<X\>\"", "0"], ",", " ", "\"\<=\>\"", ",", " ", 
        "X"}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"k", " ", "<", " ", "max"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"For", "[", " ", 
          RowBox[{
           RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
           RowBox[{"i", "\[LessEqual]", " ", "n"}], ",", " ", 
           RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            SubscriptBox["X", 
             RowBox[{"[", 
              RowBox[{"[", "i", "]"}], "]"}]], " ", "=", 
            RowBox[{
             FractionBox["1", 
              SubscriptBox["A", 
               RowBox[{"[", 
                RowBox[{"[", 
                 RowBox[{"i", ",", "i"}], "]"}], "]"}]]], 
             RowBox[{"(", 
              RowBox[{
               SubscriptBox["B", 
                RowBox[{"[", 
                 RowBox[{"[", "i", "]"}], "]"}]], " ", "+", " ", 
               RowBox[{
                SubscriptBox["A", 
                 RowBox[{"[", 
                  RowBox[{"[", 
                   RowBox[{"i", ",", "i"}], "]"}], "]"}]], " ", 
                SubscriptBox["X", 
                 RowBox[{"[", 
                  RowBox[{"[", "i", "]"}], "]"}]]}], " ", "-", " ", 
               RowBox[{
                UnderoverscriptBox["\[Sum]", 
                 RowBox[{"j", "=", "1"}], "n"], 
                RowBox[{
                 SubscriptBox["A", 
                  RowBox[{"[", 
                   RowBox[{"[", 
                    RowBox[{"i", ",", "j"}], "]"}], "]"}]], " ", 
                 SubscriptBox["X", 
                  RowBox[{"[", 
                   RowBox[{"[", "j", "]"}], "]"}]]}]}]}], ")"}]}]}]}], "]"}], 
         ";", "\[IndentingNewLine]", 
         RowBox[{"Print", "[", 
          RowBox[{
           SubscriptBox["\"\<X\>\"", 
            RowBox[{"k", "+", "1"}]], ",", " ", "\"\<=\>\"", ",", " ", "X"}], 
          "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{"Xk", " ", "=", " ", "X"}], ";", "\[IndentingNewLine]", 
         RowBox[{"k", " ", "=", " ", 
          RowBox[{"k", "+", "1"}]}], ";"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<No. of iterations performed \>\"", ",", " ", "k"}], "]"}],
       ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "X", "]"}], ";"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
