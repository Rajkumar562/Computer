Cell[BoxData[{
 RowBox[{
  RowBox[{"LUDecomp", "[", 
   RowBox[{"A0_", ",", " ", "n_"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"A", " ", "=", " ", "A0"}], ",", "i", ",", "p"}], "}"}], ",", 
    " ", 
    RowBox[{
     RowBox[{"U", " ", "=", " ", "A0"}], ";", " ", 
     RowBox[{"L", " ", "=", " ", 
      RowBox[{"IdentityMatrix", "[", "n", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{
       RowBox[{"MatrixForm", "[", "L", "]"}], ",", " ", 
       RowBox[{"MatrixForm", "[", "U", "]"}], ",", " ", "\"\< = \>\"", ",", 
       "\[IndentingNewLine]", 
       RowBox[{"MatrixForm", "[", "A0", "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"p", " ", "=", " ", "1"}], ",", " ", 
       RowBox[{"p", " ", "\[LessEqual]", 
        RowBox[{"n", "-", "1"}]}], ",", " ", 
       RowBox[{"p", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"i", " ", "=", " ", 
           RowBox[{"p", "+", "1"}]}], ",", " ", 
          RowBox[{"i", " ", "\[LessEqual]", " ", "n"}], ",", " ", 
          RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"m", "=", " ", 
            FractionBox[
             SubscriptBox["A", 
              RowBox[{"[", 
               RowBox[{"[", 
                RowBox[{"i", ",", "p"}], "]"}], "]"}]], 
             SubscriptBox["A", 
              RowBox[{"[", 
               RowBox[{"[", 
                RowBox[{"p", ",", "p"}], "]"}], "]"}]]]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{
            SubscriptBox["L", 
             RowBox[{"[", 
              RowBox[{"[", 
               RowBox[{"i", ",", "p"}], "]"}], "]"}]], "=", " ", "m"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{
            SubscriptBox["A", 
             RowBox[{"[", 
              RowBox[{"[", "i", "]"}], "]"}]], " ", "=", " ", 
            RowBox[{
             SubscriptBox["A", 
              RowBox[{"[", 
               RowBox[{"[", "i", "]"}], "]"}]], " ", "-", " ", 
             RowBox[{"m", " ", 
              SubscriptBox["A", 
               RowBox[{"[", 
                RowBox[{"[", "p", "]"}], "]"}]]}]}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"U", " ", "=", " ", "A"}], ";", "\[IndentingNewLine]", 
           RowBox[{"Print", "[", 
            RowBox[{
             RowBox[{"MatrixForm", "[", "L", "]"}], ",", " ", 
             RowBox[{"MatrixForm", "[", "U", "]"}], ",", " ", 
             "\[IndentingNewLine]", "\"\< = \>\"", ",", " ", 
             RowBox[{"MatrixForm", "[", "A0", "]"}]}], "]"}], ";"}]}], "]"}], 
        ";"}]}], "]"}], ";"}]}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Print", "[", 
   RowBox[{"\"\<L\>\"", ",", " ", "\"\<=\>\"", ",", " ", 
    RowBox[{"MatrixForm", "[", "L", "]"}]}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Print", "[", 
   RowBox[{"\"\<U\>\"", ",", " ", "\"\<=\>\"", ",", " ", 
    RowBox[{"MatrixForm", "[", "U", "]"}]}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
