Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"SimpsonRule", "[", 
    RowBox[{"a0_", ",", " ", "b0_", ",", " ", "m_", ",", "f_"}], "]"}], " ", ":=",
    " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"a", " ", "=", " ", "a0"}], ",", " ", 
       RowBox[{"b", " ", "=", " ", "b0"}], ",", "h", ",", "ApproxIntegral", 
       ",", "n"}], "}"}], ",", "\[IndentingNewLine]", 
     "                                                                  ", 
     RowBox[{
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Mod", "[", 
          RowBox[{"m", ",", "2"}], "]"}], "\[NotEqual]", "0"}], ",", 
        RowBox[{
         RowBox[{
         "Print", "[", "\"\<m should be even positive integer\>\"", "]"}], 
         ";", "\[IndentingNewLine]", 
         RowBox[{"Return", "[", "]"}], ";"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"h", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"b", "-", "a"}], ")"}], "/", "m"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"n", "=", 
       RowBox[{"m", "/", "2"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"ApproxIntegral", " ", "=", " ", 
       RowBox[{
        RowBox[{
         FractionBox["h", "3"], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"f", "[", "a", "]"}], "+", 
           RowBox[{"f", "[", "b", "]"}]}], ")"}]}], " ", "+", " ", 
        RowBox[{
         FractionBox[
          RowBox[{"2", " ", "h"}], "3"], "  ", 
         RowBox[{
          UnderoverscriptBox["\[Sum]", 
           RowBox[{"k", "=", "1"}], 
           RowBox[{"n", "-", "1"}]], 
          RowBox[{"f", "[", 
           RowBox[{"a", "+", 
            RowBox[{"2", " ", "k", " ", "h"}]}], "]"}]}]}], "+", 
        RowBox[{
         FractionBox[
          RowBox[{"4", " ", "h"}], "3"], " ", 
         RowBox[{
          UnderoverscriptBox["\[Sum]", 
           RowBox[{"k", "=", "1"}], "n"], 
          RowBox[{"f", "[", 
           RowBox[{"a", "+", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{"2", " ", "k"}], "-", "1"}], ")"}], "h"}]}], 
           "]"}]}]}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "ApproxIntegral", "]"}], ";"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], " ", ":=", " ", 
   FractionBox["1", 
    RowBox[{"x", "+", "1"}]]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"N", "[", 
