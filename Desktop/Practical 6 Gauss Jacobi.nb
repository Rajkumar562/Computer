   RowBox[{"GaussJacobi", "[", 
    RowBox[{"A0_", ",", "B0_", ",", "X0_", ",", " ", "max_"}], "]"}], " ", ":=",
    " ", "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
