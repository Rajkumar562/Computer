        RowBox[{"k", " ", "<", " ", "max"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"For", "[", " ", 
          RowBox[{
           RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
           RowBox[{"i", "\[LessEqual]", " ", "n"}], ",", " ", 
           RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            SubscriptBox["X", 
             RowBox[{"[", 
              RowBox[{"[", "i", "]"}], "]"}]], " ", "=", " ", 
            RowBox[{
