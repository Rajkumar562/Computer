Cell[CellGroupData[{

Cell[BoxData[{
 StyleBox[
  RowBox[{
   RowBox[{"Secant", "[", 
    RowBox[{"x0_", ",", "x1_", ",", "n_", ",", "f_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"xk", ",", "xk1", ",", "xk2"}], "}"}], ",", 
     RowBox[{
      RowBox[{"xk", "=", 
       RowBox[{"N", "[", "x0", "]"}]}], ";", 
      RowBox[{"xk1", "=", 
       RowBox[{"N", "[", "x1", "]"}]}], ";", 
      RowBox[{"i", "=", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Output", "=", 
       RowBox[{"{", " ", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"i", "<", "n"}], ",", 
        RowBox[{
         RowBox[{"xk2", "=", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"xk", "*", 
              RowBox[{"f", "[", "xk1", "]"}]}], "-", 
             RowBox[{"xk1", "*", 
              RowBox[{"f", "[", "xk", "]"}]}]}], ")"}], "/", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"f", "[", "xk1", "]"}], "-", 
             RowBox[{"f", "[", "xk", "]"}]}], ")"}]}]}], ";", 
         RowBox[{"interval", "=", 
          RowBox[{"\"\<[\>\"", "<>", 
           RowBox[{"ToString", "[", 
            RowBox[{"NumberForm", "[", 
             RowBox[{"xk", ",", "12"}], "]"}], "]"}], "<>", "\"\<,\>\"", "<>", 
           RowBox[{"ToString", "[", 
            RowBox[{"NumberForm", "[", 
             RowBox[{"xk1", ",", "12"}], "]"}], "]"}], "<>", "\"\<]\>\""}]}], 
         ";", 
         RowBox[{"xk", "=", "xk1"}], ";", 
         RowBox[{"xk1", "=", "xk2"}], ";", 
         RowBox[{"i", "++"}], ";", "\[IndentingNewLine]", 
         RowBox[{"Output", "=", 
          RowBox[{"Append", "[", 
           RowBox[{"Output", ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "interval", ",", "xk2", ",", 
              RowBox[{"f", "[", "xk2", "]"}]}], "}"}]}], "]"}]}], ";"}]}], 
       "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"NumberForm", "[", 
        RowBox[{
         RowBox[{"TableForm", "[", 
          RowBox[{"Output", ",", 
           RowBox[{"TableHeadings", "\[Rule]", 
            RowBox[{"{", 
             RowBox[{"None", ",", 
              RowBox[{"{", 
               RowBox[{
               "\"\<i\>\"", ",", "\"\<interval\>\"", ",", "\"\<xi\>\"", ",", 
                "\"\<f[xi]\>\""}], "}"}]}], "}"}]}]}], "]"}], ",", "8"}], 
        "]"}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{
       "\"\< Root after \>\"", ",", "n", ",", "\"\< iterations \>\"", ",", 
        RowBox[{"NumberForm", "[", 
         RowBox[{"xk2", ",", "8"}], "]"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\< Function value at approximated root,f[xi]= \>\"", ",", 
        RowBox[{"NumberForm", "[", 
         RowBox[{
          RowBox[{"f", "[", "xk2", "]"}], ",", "8"}], "]"}]}], "]"}], ";"}]}],
     "]"}]}],
  FontFamily->"Courier New",
  FontSize->18], "\[IndentingNewLine]", 
 StyleBox[
  RowBox[{
   RowBox[{
    RowBox[{"g", "[", "x_", "]"}], ":=", 
    RowBox[{
     SuperscriptBox["x", "3"], "-", 
     RowBox[{"5", "x"}], "+", "1"}]}], ";"}],
  FontFamily->"Courier New",
