Cell[BoxData[{
 StyleBox[
  RowBox[{
   RowBox[{"Bisection", "[", 
    RowBox[{"a0_", ",", "b0_", ",", "n_", ",", "f_"}], "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"Module", " ", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"a", " ", "=", " ", 
        RowBox[{"N", "[", "a0", "]"}]}], ",", 
       RowBox[{"b", "=", 
        RowBox[{"N", "[", "b0", "]"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"c", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"a", "+", "b"}], ")"}], "/", "2"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"i", "=", 
       RowBox[{"0", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"f", "[", "a", "]"}], "*", 
            RowBox[{"f", "[", "b", "]"}]}], ">", "0"}], ",", 
          RowBox[{
           RowBox[{"Print", "[", 
            RowBox[{
            "\"\<we cannot continue with the bisection method as \>\"", ",", 
             "f", ",", "\"\<(a).\>\"", ",", "f", ",", "\"\<(b)>=0\>\""}], 
            "]"}], ";", 
           RowBox[{"Return", "[", "]"}]}]}], "]"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"output", "=", 
       RowBox[{"{", 
        RowBox[{"{", 
         RowBox[{"i", ",", "a", ",", "c", ",", "b", ",", 
          RowBox[{"f", "[", "c", "]"}]}], "}"}], "}"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"i", "<", "n"}], ",", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Sign", "[", 
             RowBox[{"f", "[", "b", "]"}], "]"}], "==", 
            RowBox[{"Sign", "[", 
             RowBox[{"f", "[", "c", "]"}], "]"}]}], ",", 
           RowBox[{"b", "=", "c"}], ",", 
           RowBox[{
            RowBox[{"a", "=", "c"}], ";"}]}], "]"}], ";", 
         RowBox[{"c", "=", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"a", "+", "b"}], ")"}], "/", "2"}]}], ";", 
         RowBox[{"i", "=", 
          RowBox[{"i", "+", "1"}]}], ";", " ", 
         RowBox[{"output", "=", 
          RowBox[{"Append", "[", 
           RowBox[{"output", ",", " ", 
            RowBox[{"{", 
             RowBox[{"i", ",", "a", ",", "c", ",", "b", ",", 
              RowBox[{"f", "[", "c", "]"}]}], "}"}]}], "]"}]}], ";"}]}], 
       "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"NumberForm", "[", 
        RowBox[{
         RowBox[{"TableForm", "[", 
          RowBox[{"output", ",", 
           RowBox[{"TableHeadings", "\[Rule]", 
            RowBox[{"{", 
             RowBox[{"None", ",", 
              RowBox[{"{", 
               RowBox[{
               "\"\<i\>\"", ",", "\"\<\!\(\*SubscriptBox[\(a\), \(i\)]\)\>\"",
                 ",", "\"\<\!\(\*SubscriptBox[\(c\), \(i\)]\)\>\"", ",", 
                "\"\<\!\(\*SubscriptBox[\(b\), \(i\)]\)\>\"", ",", 
                "\"\<f[\!\(\*SubscriptBox[\(c\), \(i\)]\)]\>\""}], "}"}]}], 
             "}"}]}]}], "]"}], ",", "16"}], "]"}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{
       "\"\<Approximate root after \>\"", ",", "i", ",", 
        "\"\< iterations is \>\"", ",", 
        RowBox[{"NumberForm", "[", 
         RowBox[{"c", ",", "16"}], "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Function value at approximated root,f[c]=\>\"", ",", 
        RowBox[{"NumberForm", "[", 
         RowBox[{
          RowBox[{"f", "[", "c", "]"}], ",", "16"}], "]"}]}], "]"}], ";"}]}], 
    "]"}]}],
