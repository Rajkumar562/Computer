Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"TrapezoidRule", "[", 
    RowBox[{"a0_", ",", "b0_", ",", "n_", ",", "f_"}], "]"}], ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"a", "=", "a0"}], ",", 
       RowBox[{"b", "=", "b0"}], ",", "h", ",", "ApproxIntegral"}], "}"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"h", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"b", "-", "a"}], ")"}], "/", "n"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"ApproxIntegral", "=", " ", 
       RowBox[{
        RowBox[{
         FractionBox["h", "2"], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"f", "[", "a", "]"}], "+", 
           RowBox[{"f", "[", "b", "]"}]}], ")"}]}], "+", 
        RowBox[{"h", " ", 
         RowBox[{
          UnderoverscriptBox["\[Sum]", 
           RowBox[{"k", "=", "1"}], 
           RowBox[{"n", "-", "1"}]], 
          RowBox[{"f", "[", 
           RowBox[{"a", "+", 
            RowBox[{"h", " ", "k"}]}], "]"}]}]}]}]}], ";", " ", 
      RowBox[{"Return", "[", "ApproxIntegral", "]"}], ";"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], ":=", " ", 
   RowBox[{"Log", "[", "x", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"TrapezoidRule", "[", 
