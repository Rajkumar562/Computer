import java.util.*;
public class Specialf
{
    int n;
    public Specialf(int x)
    {
        n=x;
    }
    public void isSpecial()
    {
        int a=n;
        int f,sum=0;
        while(a>0)
        {
            int fact=1;
            f=a%10;
            for(int i=1;i<=f;i++)
            {
                fact=fact*i;
            }
            sum=sum+fact;
            a=a/10;
        }
        if(sum==n)
        System.out.print("Special Number");
        else
        System.out.print("Not a Special Number");
    }
    public static void main()
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter a number to be checked");
        int a1=sc.nextInt();
        Specialf sf=new Special(a1);
        sf.isSpecial();
    }
}