public class Compare
{
    public static void main(int a,int b,int c)
    {
        int x=0;
        int y=0;
        if(a==b)
        {
            if(a<c)
            {a=x;
            c=y;
            a=y;
            c=x;}
        }
        if(a==c)
        {if(a<b)
            {a=x;
                b=y;
                a=y;
                b=x;
            }
        }
        if(a<b)
        {
            a=x;
            b=y;
            a=y;
            b=x;
        }
        if(a<c)
        {
            a=x;
            c=y;
            a=y;
            c=x;
        }
        if(b<c)
        {
            b=x;
            c=y;
            b=y;
            c=x;
        }
        System.out.println("Descending Order is "+a+" "+b+" "+c);
    }
}